{
  "name": "stenciljs-tailwindcss",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "NODE_ENV=production stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test:unit": "stencil test --spec",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.3",
    "@stencil/postcss": "^1.0.1",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^3.0.2",
    "jest": "^26.6.0",
    "jest-cli": "^26.6.0",
    "puppeteer": "^5.3.1",
    "tailwindcss": "^1.9.5",
    "workbox-build": "^5.1.4"
  },
  "license": "MIT"
}
